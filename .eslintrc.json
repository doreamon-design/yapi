{
    "env": {
        "browser": true,
        "node": true,
        "commonjs": true
    },
    "extends": [],
    "globals": [
        "$",
        "$$",
        "jQuery",
        "Promise"
    ],
    "rules": {
        "no-with": "error",
        "no-octal": "error",
        "no-undef": "error",
        "no-dupe-keys": "error",
        "no-dupe-args": "error",
        "no-delete-var": "error",
        "no-unused-vars": "error",
        "no-caller": "error",
        "no-debugger": "error",
        "no-unreachable": "error",
        "no-inner-declarations": [
            "error",
            "functions"
        ]
    },
    "ignorePattern": [
        "/fekit_modules/*",
        "/node_modules/*",
        "/bower_components/*",
        "/plugins/*",
        "/dev/*",
        "/prd/*"
    ]
}
